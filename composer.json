{
    "name": "m-porter/vue-template-loader",
    "description": "Laravel package which will download your html templates as built by webpack-html-loader for use in your projects.",
    "keywords": ["vue", "laravel", "webpack", "html", "loader"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Porter",
            "email": "mtp5129@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5,<5.9",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "MPorter\\VueTemplateLoader\\": "src/"
        }
    },
    "scripts": {
        "lint": "phpcs -pns --standard=PSR2 src/ config/",
        "lint-fix": "phpcbf -pns --standard=PSR2 src/ config/"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MPorter\\VueTemplateLoader\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
